{
  "name": "YouTrackMobile",
  "version": "0.1.0-0",
  "author": "JetBrains",
  "bugs": {
    "url": "https://youtrack.jetbrains.com/issues/YTM",
    "email": "youtrack-feedback@jetbrains.com"
  },
  "config": {
    "ANALYTICS_ID": "",
    "EXCEPTION_REPORTER_TOKEN": "",
    "KONNECTOR_URL": ""
  },
  "bugsnag": {
    "version": "0.1.0",
    "token": " ",
    "uploadurl": "https://upload.bugsnag.com/react-native-source-map"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn run lint && yarn run test-dev-data && yarn test"
    }
  },
  "license": "Apache-2.0",
  "scripts": {
    "android": "YT_MOBILE_VERSION=$npm_package_version react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "yarn test-dev-data && jest",
    "lint__": "eslint .",
    "android__": "YT_MOBILE_VERSION=$npm_package_version ./node_modules/react-native-cli/index.js run-android",
    "android-align-after-sign": "$ANDROID_HOME/build-tools/28.0.3/zipalign -v 4 android/app/build/outputs/apk/release/app-release-unsigned.apk android/app/build/outputs/apk/release/app-release.apk",
    "android-bundle-release": "cd android && YT_MOBILE_VERSION=$npm_package_version ./gradlew bundleRelease && cd ..",
    "android-clean": "cd ./android && YT_MOBILE_VERSION=$npm_package_version ./gradlew clean && cd ..",
    "android-develop": "yarn start & yarn run android-emulator & yarn run android",
    "android-emulator": "script/android-emulator.sh",
    "android-release": "cd android && YT_MOBILE_VERSION=$npm_package_version ./gradlew assembleRelease && cd ..",
    "bugsnag:android:generate-sourcemap": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output $npm_package_tmp/android-release.bundle --sourcemap-output $npm_package_tmp/android-release.bundle.map",
    "bugsnag:android:upload-sourcemap": "wait-on $npm_package_tmp/android-release.bundle.map && curl $npm_package_bugsnag_uploadurl -F overwrite=true -F apiKey=$npm_package_bugsnag_token -F appVersion=$npm_package_bugsnag_version -F dev=false -F platform=android -F sourceMap=@$npm_package_tmp/android-release.bundle.map -F bundle=@$npm_package_tmp/android-release.bundle -F projectRoot=`pwd`",
    "bugsnag:ios:generate-sourcemap": "react-native bundle --platform ios --dev false --entry-file index.js --bundle-output $npm_package_tmp/ios-release.bundle --sourcemap-output $npm_package_tmp/ios-release.bundle.map",
    "bugsnag:ios:upload-sourcemap": "wait-on $npm_package_tmp/ios-release.bundle.map && curl $npm_package_bugsnag_uploadurl -F overwrite=true -F apiKey=$npm_package_bugsnag_token -F appVersion=$npm_package_bugsnag_version -F dev=false -F platform=ios -F sourceMap=@$npm_package_tmp/ios-release.bundle.map -F bundle=@$npm_package_tmp/ios-release.bundle -F projectRoot=`pwd`",
    "bugsnag:upload": "yarn run clearTmp && yarn run bugsnag:android:generate-sourcemap && yarn run bugsnag:android:upload-sourcemap && yarn run bugsnag:ios:generate-sourcemap && yarn run bugsnag:ios:upload-sourcemap",
    "resetEnv": "watchman watch-del-all && rm -rf $TMPDIR/react-* && rm -rf $TMPDIR/metro-* && rm -rf $TMPDIR/haste-* && yarn cache clean && jest --clearCache",
    "clearTmp": "rm -rf $npm_package_tmp && mkdir $npm_package_tmp",
    "copy-font": "cp resources/fonts/youtrackmobile.ttf ./ios/ && cp resources/fonts/youtrackmobile.ttf ./android/app/src/main/assets/fonts/ && cp resources/fonts/selection.json ./src/components/icon/",
    "debug-test": "node --inspect-brk ./node_modules/.bin/jest --runInBand",
    "e2e-build:ios": "detox build --configuration ios.sim.release",
    "e2e-clean": "detox clean-framework-cache && detox build-framework-cache",
    "e2e-shutdown": "fbsimctl shutdown",
    "e2e-test:ios": "detox test --configuration ios.sim.release --debug-synchronization 200 --detectOpenHandles --artifacts-location ./e2e/logs/ --loglevel error --record-logs failing --jest-report-specs --cleanup  --bail",
    "e2e:ios": "yarn run e2e-clean && yarn run e2e-build:ios && yarn run e2e-test:ios",
    "e2e:ci": "yarn run ios-delete-build && script/ci-e2e-runner.sh",
    "flow-watch": "fswatch -o ./ | xargs -n1 -I{} sh -c 'clear; printf \"\\033[3J\" && flow'",
    "ios__": "./node_modules/react-native-cli/index.js run-ios --simulator='iPhone 6'",
    "ios-archive": "xcodebuild archive -allowProvisioningUpdates -workspace ios/YouTrackMobile.xcworkspace -scheme YouTrackMobile[Release] -archivePath ios/build/YouTrackMobile[Release].xcarchive",
    "ios-archive-no-sign": "export RCT_NO_LAUNCH_PACKAGER=true && yarn run ios-archive -- CODE_SIGN_IDENTITY='' CODE_SIGNING_REQUIRED=NO CODE_SIGNING_ALLOWED='NO'",
    "ios-build-bundle": "react-native bundle --entry-file=index.js --platform ios --dev false --reset-cache --bundle-output build/main.jsbundle --assets-dest build",
    "ios-iPadAir": "./node_modules/react-native-cli/index.js run-ios --simulator='iPad Air'",
    "ios-iphoneX": "./node_modules/react-native-cli/index.js run-ios --simulator='iPhone X'",
    "ios-iphone8Plus": "./node_modules/react-native-cli/index.js run-ios --simulator='iPhone 8 Plus (13.5)'",
    "ios-release": "yarn run ios-download-archive && yarn run ios-export-ipa",
    "ios-set-version": "node script/set-ios-version",
    "flow-check": "flow check",
    "lint": "eslint src",
    "postinstall": "script/postinstall.sh",
    "postios-archive-no-sign": "cp ios/build/YouTrackMobile[Release].xcarchive/Products/Applications/YouTrackMobile.app/YouTrackMobile.entitlements ios/build/YouTrackMobile[Release].xcarchive/Products/Applications/YouTrackMobile.app/archived-expanded-entitlements.xcent",
    "prepare-config": "node script/prepare-config",
    "test-dev-data": "node script/test-dev-data.js",
    "test__": "yarn test-dev-data && jest",
    "test-set-env": "DETOX_DISABLE_POSTINSTALL=true TEAMCITY_VERSION=enablereport",
    "test:watch": "yarn test -- --watchAll",
    "ios-clean": "xcodebuild clean -workspace ios/YouTrackMobile.xcworkspace -configuration Release -alltargets",
    "ios-delete-build": "rm -rf ios/build/",
    "ios-export-ipa": "xcodebuild -exportArchive -allowProvisioningUpdates -archivePath ios/build/YouTrackMobile[Release].xcarchive -exportPath ios/build -exportOptionsPlist script/export.plist"
  },
  "tmp": ".tmp",
  "dependencies": {
    "@expo/react-native-action-sheet": "3.8.0",
    "@gpsgate/react-native-eventsource": "ExpandoPakistan/react-native-eventsource.git",
    "@jetbrains/icons": "^3.14.1",
    "@react-native-community/async-storage": "^1.4.0",
    "@react-native-community/netinfo": "^6.0.0",
    "@reduxjs/toolkit": "^1.4.0",
    "base64-js": "1.5.1",
    "bugsnag-react-native": "^2.23.10",
    "diff-match-patch": "^1.0.5",
    "entities": "^1.1.1",
    "from-now": "1.0.2",
    "htmlparser-to-html": "0.0.6",
    "jetifier": "^1.6.6",
    "lodash.once": "^4.1.1",
    "lodash.throttle": "^4.1.1",
    "markdown-it-checkbox": "anisimov74/markdown-it-checkbox.git#5f35851",
    "object-walk": "^0.1.1",
    "performant-array-to-tree": "^1.8.1",
    "qs": "6.10.1",
    "react": "17.0.1",
    "react-dom": "^17.0.2",
    "react-fast-compare": "^3.2.0",
    "react-native": "0.64.1",
    "react-native-animatable": "1.2.4",
    "react-native-appearance": "0.3.4",
    "react-native-calendars": "1.5.3",
    "react-native-device-info": "1.6.2",
    "react-native-device-log": "Trihydro/react-native-device-log",
    "react-native-easy-toast": "^2.0.0",
    "react-native-encrypted-storage": "^4.0.2",
    "react-native-extended-stylesheet": "^0.12.0",
    "react-native-gesture-handler": "1.10.3",
    "react-native-google-analytics": "1.3.2",
    "react-native-htmlview": "0.13.0",
    "react-native-hyperlink": "anisimov74/react-native-hyperlink.git#aaa91f8",
    "react-native-image-crop-picker": "0.36.0",
    "react-native-image-gallery": "Elvenisboy/react-native-image-gallery.git#9282a87",
    "react-native-image-progress": "1.0.1",
    "react-native-input-scroll-view": "^1.11.0",
    "react-native-keyboard-spacer": "0.4.1",
    "react-native-keychain": "1.0.0",
    "react-native-linear-gradient": "2.4.0",
    "react-native-markdown-display": "^6.1.6",
    "react-native-navigation-bar-color": "^2.0.1",
    "react-native-notifications": "anisimov74/react-native-notifications.git#fd47b17",
    "react-native-pager-view": "^5.1.2",
    "react-native-reanimated": "^2.1.0",
    "react-native-restart": "^0.0.22",
    "react-native-safe-area-context": "^3.2.0",
    "react-native-screens": "^2.18.1",
    "react-native-skeleton-placeholder": "^2.0.7",
    "react-native-svg": "^9.13.3",
    "react-native-svg-transformer": "^0.14.3",
    "react-native-swiper": "^1.6.0",
    "react-native-switch-pro": "^1.0.4",
    "react-native-syntax-highlighter": "1.2.6",
    "react-native-tab-view": "3.0.0",
    "react-native-vector-icons": "7.1.0",
    "react-navigation": "^3.11.0",
    "react-navigation-stack": "^1.4.0",
    "react-redux": "^7.2.2",
    "react-syntax-highlighter": "^5.6.2",
    "redux": "4.0.5",
    "redux-create-reducer": "2.0.1",
    "redux-thunk": "2.3.0",
    "url-join": "4.0.0",
    "url-parse": "1.5.0",
    "use-debounce": "^5.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "@testing-library/jest-native": "^4.0.1",
    "@testing-library/react-native": "^7.2.0",
    "babel-jest": "^26.6.3",
    "chai": "3.5.0",
    "chai-as-promised": "6.0.0",
    "chai-enzyme": "1.0.0-beta.0",
    "cheerio": "0.22.0",
    "core-util-is": "1.0.2",
    "detox": "^18.8.1",
